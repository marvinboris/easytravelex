{
  "name": "easytravelex",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "clean": "del-cli ./dist",
    "dev": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/app.ts",
    "build": "tsc",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:coverage": "npm run test -- --coverage --coverageReporters=\"json-summary\"",
    "test:badges": "npm run test:coverage && jest-badge-generator",
    "lint": "eslint ./src --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "update-coverage": "ts-node update-readme-coverage.ts",
    "prepack": "npm run build",
    "posttest": "npm run update-coverage"
  },
  "keywords": [],
  "author": "marvinboris",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "mongoose": "^7.6.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.19",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.5",
    "@types/morgan": "^1.9.6",
    "@types/supertest": "^2.0.14",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.4",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "jest-badge-generator": "^1.1.5",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}
